<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.hendyirawan.lewattol" version="0.4.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>JETI Jakarta Transit</name>
  <description>
        Solution for Jakarta netizen. Bus and train navigation for people on the go!
    </description>
  <author email="ceefour666@gmail.com" href="http://www.hendyirawan.com/">
      Hendy Irawan
    </author>
  <content src="index.html"/>
	<plugin name="cordova-plugin-whitelist" spec="1" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <icon src="icon/icon-128.png" width="128" height="128"/>
  <platform name="android">
        <allow-intent href="market:*" />
    <icon src="res/android/xhdpi.png" density="xhdpi"/>
  </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
